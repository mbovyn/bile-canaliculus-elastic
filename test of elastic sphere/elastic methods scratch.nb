Cell[CellGroupData[{

Cell["\<\
relationship between 3d vectors and 2d vectors for general_linear_elastic\
\>", "Section",
 CellChangeTimes->{{3.853498126734462*^9, 3.853498153063499*^9}, {
  3.8539192709975767`*^9, 
  3.853919274730775*^9}},ExpressionUUID->"342169f7-1460-4e87-9e5d-\
4cf8146cd89a"],

Cell["\<\
We have some vectors for the sides (v1,v2) which are 3D and some vectors we \
need (u1,u2) which are 2D\
\>", "Text",
 CellChangeTimes->{{3.853919278139312*^9, 
  3.853919328347549*^9}},ExpressionUUID->"c294d065-8ac3-497c-a6b6-\
01b088769821"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"v1", "=", 
   RowBox[{"{", 
    RowBox[{"x1", ",", "y1", ",", "z1"}], "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"v2", "=", 
   RowBox[{"{", 
    RowBox[{"x2", ",", "y2", ",", "z2"}], "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"u1", "=", 
   RowBox[{"{", 
    RowBox[{"a1", ",", "0"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"u2", "=", 
   RowBox[{"{", 
    RowBox[{"a2", ",", "b2"}], "}"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.853498158936878*^9, 3.853498210649364*^9}, {
  3.853498500835609*^9, 3.8534985035932713`*^9}},
 CellLabel->"In[1]:=",ExpressionUUID->"774c07cc-81c4-4fe9-a37c-83e005de4c80"],

Cell["\<\
Impose that the length of v1 and u1 must be the same (also for v2 and u2), \
and that the dot products between the vectors must be the same.
If we are completely general, get lots of answers, so force b2=0 to reduce \
the space.\
\>", "Text",
 CellChangeTimes->{{3.853919349897706*^9, 
  3.8539194448159018`*^9}},ExpressionUUID->"40865516-4e76-4017-af3d-\
ca44b6b6cc22"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"compsol", "=", 
  RowBox[{
   RowBox[{
    RowBox[{"Solve", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{"Norm", "[", "v1", "]"}], "==", 
         RowBox[{"Norm", "[", "u1", "]"}]}], ",", 
        RowBox[{
         RowBox[{"Norm", "[", "v2", "]"}], "==", 
         RowBox[{"Norm", "[", "u2", "]"}]}], ",", 
        RowBox[{
         RowBox[{"v1", ".", "v2"}], "==", 
         RowBox[{"u1", ".", "u2"}]}]}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"a1", ",", "b1", ",", "a2", ",", "b2"}], "}"}]}], "]"}], "/.", 
    
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       SqrtBox[
        RowBox[{
         SuperscriptBox[
          RowBox[{"Abs", "[", "x1", "]"}], "2"], "+", 
         SuperscriptBox[
          RowBox[{"Abs", "[", "y1", "]"}], "2"], "+", 
         SuperscriptBox[
          RowBox[{"Abs", "[", "z1", "]"}], "2"]}]], "->", "v1n"}], ",", 
      RowBox[{
       RowBox[{
        SuperscriptBox[
         RowBox[{"Abs", "[", "x1", "]"}], "2"], "+", 
        SuperscriptBox[
         RowBox[{"Abs", "[", "y1", "]"}], "2"], "+", 
        SuperscriptBox[
         RowBox[{"Abs", "[", "z1", "]"}], "2"]}], "->", "v1n2"}], ",", 
      RowBox[{
       RowBox[{
        SuperscriptBox[
         RowBox[{"Abs", "[", "x2", "]"}], "2"], "-", " ", 
        SuperscriptBox[
         RowBox[{"Abs", "[", "y2", "]"}], "2"], "-", " ", 
        SuperscriptBox[
         RowBox[{"Abs", "[", "z2", "]"}], "2"]}], "->", "v2n2"}], ",", 
      RowBox[{
       RowBox[{
        RowBox[{"x1", " ", "x2"}], "+", 
        RowBox[{"y1", " ", "y2"}], "+", 
        RowBox[{"z1", " ", "z2"}]}], "->", "v1dv2"}]}], "}"}]}], "//", 
   "Simplify"}]}]], "Input",
 CellChangeTimes->{{3.853498217608652*^9, 3.8534982549344063`*^9}, {
  3.853498285484983*^9, 3.853498326516953*^9}, {3.8534985938595247`*^9, 
  3.853498679530242*^9}, {3.85349874534939*^9, 3.853498750804452*^9}, {
  3.8534988255346413`*^9, 3.853498828951174*^9}, {3.853919476079624*^9, 
